.searchControl
  background: #f4f2ff;
  border: 1px solid rgba(0,0,0,0);
  box-shadow: inset 0 1px 4px rgba(0,0,0,0.2);
  color: #6e6893;
  font-size: 12px;
  padding: 8px 8px 8px 40px;
  width: calc(100% - 20px);
  margin-left 20px
  &:focus
    border: 1px solid #6D5BD0;
  &Wrap
    position relative
    margin-bottom: 8px;
  &Icon
    position: absolute;
    left: 30px;
    top: 0;
    bottom: 0;
    margin: auto 0;
    display: flex;
    justify-content: center;
    align-items: center;
  &::-webkit-input-placeholder
    font-size 16px
    color: #25213B;
  &::-moz-placeholder
    font-size 16px
    color: #25213B;
  &:-ms-input-placeholder
    font-size 16px
    color: #25213B;
  &:-moz-placeholder
    font-size 16px
    color: #25213B;
